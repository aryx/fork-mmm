  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=no"
fi
rm -f conftest*
LIBS="$ac_save_LIBS"

fi
if eval "test \"`echo '$ac_cv_lib_'$ac_lib_var`\" = yes"; then
  echo "$ac_t""yes" 1>&6
  LIBS="-l$tcld $LIBS"
else
  echo "$ac_t""no" 1>&6
echo $ac_n "checking for -l$tcl""... $ac_c" 1>&6
