index:V:
	$INDEXER -lang cmt $MMMDIR \
    | sed -e 's#main.cmt#Main#' \
    | grep -v 'lib_core/' \
    | grep -v 'external/ocamltk' \
    | grep -v 'i18n/japan' \
    | grep -v 'tk/' \
    | grep -v 'Common.=~' \
    | grep -v 'Common.=:=' \
    > defs_and_uses.list




